{
  "name": "holyshared/hhpack-codegen-example",
  "type": "library",
  "keywords": [ "hack", "hhvm", "package", "generator" ],
  "description": "hhpack/codegen example",
  "license": "MIT",
  "authors": [
    {
      "name": "Noritaka Horio",
      "email": "holy.shared.design@gmail.com"
    }
  ],
  "require": {
    "hhvm": ">=3.21.0",
    "hhvm/hhvm-autoload": "^1.5"
  },
  "require-dev": {
    "hackpack/hackunit": "^1.1",
    "hhpack/codegen": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "HHPack\\Example\\": "src"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/holyshared/HackUnit"
    }
  ],
  "minimum-stability": "stable",
  "scripts": {
    "test": "vendor/bin/hackunit test",
    "check:init": "touch .hhconfig"
  }
}
